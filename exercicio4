    <script>
        var  num;
        num=prompt(" Digite um número: ");
        num=parseFloat(num);
        if(num > 20 && num < 90){
            alert(" O número está entre 20 e 90 ");
        }
        else{
            alert(" O número não está  entre 20 e 90 ");
        }

    </script>

